#! /c/iverilog/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_00704A90 .scope module, "first_counter_tb" "first_counter_tb" 2 2;
 .timescale 0 0;
v0070D638_0 .var "clock", 0 0;
v0070D690_0 .net "counter_out", 3 0, v0070D530_0; 1 drivers
v0070D6E8_0 .var "enable", 0 0;
v0070D740_0 .var "reset", 0 0;
S_00703000 .scope module, "U_counter" "first_counter" 2 28, 3 9, S_00704A90;
 .timescale 0 0;
v0070ACD8_0 .net "clock", 0 0, v0070D638_0; 1 drivers
v0070D530_0 .var "counter_out", 3 0;
v0070D588_0 .net "enable", 0 0, v0070D6E8_0; 1 drivers
v0070D5E0_0 .net "reset", 0 0, v0070D740_0; 1 drivers
E_0070D998 .event posedge, v0070ACD8_0;
S_00703088 .scope begin, "COUNTER" "COUNTER" 3 35, 3 35, S_00703000;
 .timescale 0 0;
    .scope S_00703000;
T_0 ;
    %wait E_0070D998;
    %fork t_1, S_00703088;
    %jmp t_0;
    .scope S_00703088;
t_1 ;
    %load/v 8, v0070D5E0_0, 1;
    %jmp/0xz  T_0.0, 8;
    %ix/load 0, 4, 0;
    %assign/v0 v0070D530_0, 1, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/v 8, v0070D588_0, 1;
    %jmp/0xz  T_0.2, 8;
    %load/v 8, v0070D530_0, 4;
    %mov 12, 0, 28;
    %addi 8, 1, 32;
    %ix/load 0, 4, 0;
    %assign/v0 v0070D530_0, 1, 8;
T_0.2 ;
T_0.1 ;
    %end;
    .scope S_00703000;
t_0 %join;
    %jmp T_0;
    .thread T_0;
    .scope S_00704A90;
T_1 ;
    %vpi_call 2 9 "$display", "time\011 clk reset enable counter";
    %vpi_call 2 10 "$monitor", "%g\011 %b   %b     %b      %b", $time, v0070D638_0, v0070D740_0, v0070D6E8_0, v0070D690_0;
    %set/v v0070D638_0, 1, 1;
    %set/v v0070D740_0, 0, 1;
    %set/v v0070D6E8_0, 0, 1;
    %delay 5, 0;
    %set/v v0070D740_0, 1, 1;
    %delay 10, 0;
    %set/v v0070D740_0, 0, 1;
    %delay 10, 0;
    %set/v v0070D6E8_0, 1, 1;
    %delay 100, 0;
    %set/v v0070D6E8_0, 0, 1;
    %delay 5, 0;
    %vpi_call 2 19 "$finish";
    %end;
    .thread T_1;
    .scope S_00704A90;
T_2 ;
    %delay 5, 0;
    %load/v 8, v0070D638_0, 1;
    %inv 8, 1;
    %set/v v0070D638_0, 8, 1;
    %jmp T_2;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "first_counter_tb.v";
    "./first_counter.v";
